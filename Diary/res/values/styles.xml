        <item name="colorAccent">@color/UIMaterialLightGreen</item>

        <item name="android:textColorPrimary">@color/UIDarkOnyx</item>
        <item name="android:textColorPrimaryInverse">@color/Peach</item>
        <item name="android:textColorSecondary">@color/Non_Important_Text</item>
        <item name="android:textColorSecondaryInverse">@color/Peach_Pressed</item>

    </style>
