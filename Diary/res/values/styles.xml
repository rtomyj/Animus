        <item name = "android:textColorPrimary">@color/UIDarkText</item>
        <item name = "android:textColorSecondary">@color/UIDarkText</item>


        <item name="android:navigationBarColor">@color/UIDarkBlue</item>

    </style>


    <style name="Material" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="android:colorPrimary">@color/UIMaterialPink</item>
