        <item name ="android:textColor">@color/UIDarkBlue</item>


        <item name="android:navigationBarColor">@color/UIDarkBlue</item>


    </style>

    <style name="Onyx" parent="@android:style/Theme.Material">

        <item name="android:colorPrimary">@color/DarkWhite_ish</item>
        <item name="android:colorPrimaryDark">@color/DarkWhite_ish</item>
